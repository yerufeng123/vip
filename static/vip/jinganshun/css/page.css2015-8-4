.ba_btn{width:134px;height:38px;text-align:center;line-height:38px;background:#cd0000;color:#fff;font-size:13px;display:block;margin:14px auto;-webkit-border-radius:6px;}
.first_ba_tit{margin-top:50px;text-indent:23px;}
