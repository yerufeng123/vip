/***************************** 杜鹃-----活动描述文字部分6.23 *************************************/
.wrap_act1{background:url(../images/wrap_act_bg.jpg) no-repeat center;height:100%;background-size:cover;}
.wrap_act2{background:url(../images/wrap_act_bg.jpg) no-repeat center;background-size:cover;}
.close_btn{width:16px;height:16px;position:absolute;right:10px;top:6px;}
.act_det{width:100%;margin:0 auto;text-align:left;padding-top:20px;padding-left:32px;}
.act_det label{color:#5d5d5d;font-size:13px;line-height:35px;}
.act_det input{width:-webkit-calc( 80% - 12px );width:calc( 80% - 12px );height:38px;border-radius:3px;border:solid 1px #b5b5b5;background:rgba(255,255,255,.75);margin-bottom:20px;padding:0 5px;   white-space: nowrap; text-overflow: ellipsis; overflow: hidden; }
.act_det textarea{width:-webkit-calc( 80% - 12px );width:calc( 80% - 12px );height:173px;border-radius:3px;border:solid 1px #b5b5b5;background:rgba(255,255,255,.75);padding:5px 5px 0 5px;line-height:19px;text-indent:12px;}
.apply_btn.act_save_btn1,.apply_btn.act_save_btn2{margin:15px auto 0;display:inline-block;}
.apply_btn.act_save_btn2{margin-left:6%;}

.act_det a:nth-child(9){margin-left:32px;}
.act_det a:nth-child(8){margin-left:-7px;}

/***************************** 杜鹃-----活动描述文字部分--最佳员工6.23 *************************************/
.wrap_act h6{font-size:14px;color:#cd0000;text-align:center;line-height:35px;margin:0 auto;}
.act_inf{background:rgba(255,255,255,.5);}
.h6_tit{font-size:14px;text-indent:5%;}
.act_inf:last-child{padding-bottom:30px;}
.act_inf ul li{width:100%; height:110px;border-top:solid 1px #c7c7c7}
.act_inf ul li:last-child{border-bottom:solid 1px #c7c7c7}
.header_l{width:75px;float:left;padding:8px 0 5px 15px;}
.header_box{width:60px;height:60px;border:solid 3px #d32f2f;border-radius:50%;}
.header_box img{width:60px;height:60px;border-radius:50%;}
.staff_name{width:59px;height:22px;background:url(../images/text_box.png) no-repeat center;background-size:59px 22px;text-align:center;color:#fff;font-size:14px;line-height:22px;display:block;margin:8px 0 0 5px}
.act_det_r{width:-webkit-calc( 100% - 110px );float:right;padding:12px 5px 0 5px;line-height:22px;color:#303030;font-size:12px;text-indent:12px;}

/***************************** 分享出页面 *************************************/
.er_bg_send{width:226px;height:114px;margin:-50px auto -100px;padding-bottom:100px;display:block;}
.add_share_two{margin-top:10px;}




